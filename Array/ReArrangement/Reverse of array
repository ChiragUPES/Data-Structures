import java.util.*;
public class HelloWorld{

     public static void main(String []args){
        int[] arr={4,5,1,2,4,5,6};
        int strt=0;
        int end=arr.length-1;
        while(strt<end)
        {
            int temp=arr[strt];
            arr[strt]=arr[end];
            arr[end]=temp;
            strt++;
            end--;
        }
        for(int i=0;i<arr.length;i++)
        {
            System.out.println(arr[i]);
        }
     }
}
